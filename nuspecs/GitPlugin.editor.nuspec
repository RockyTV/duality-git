        <dependency id="AdamsLair.WinForms" version="1.0.16" />
        <dependency id="LibGit2Sharp" version="0.21.0.176" />
    </dependencies>
  </metadata>
  <files>
      <file src="..\EditorPlugin\bin\Release\GitPlugin.editor.dll" target="lib" />
      <file src="..\EditorPlugin\bin\Release\GitPlugin.editor.pdb" target="lib" />
      <file src="..\EditorPlugin\bin\Release\GitPlugin.editor.xml" target="lib" />
  </files>
</package>
