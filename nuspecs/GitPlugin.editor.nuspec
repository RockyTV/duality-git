<?xml version="1.0"?>
<package>
  <metadata>
    <id>RockyTV.GitPlugin.editor</id>
    <title>RockyTV's Git Plugin</title>
    <version>1.1.2</version>
    <authors>Alexandre Oliveira</authors>
    <owners>Alexandre Oliveira</owners>
    <licenseUrl>https://github.com/RockyTV/duality-git/blob/master/LICENSE</licenseUrl>
    <projectUrl>https://github.com/RockyTV/duality-git</projectUrl>
    <iconUrl>https://github.com/RockyTV/duality-git/raw/master/nuspecs/git.png</iconUrl>
    <requireLicenseAcceptance>false</requireLicenseAcceptance>
    <description>
      This plugin initializes a Git repository on your project folder, allowing you to keep track of which files were modified and what was changed in them.
    </description>
    <summary>Git version control system for Duality.</summary>
    <tags>Duality Plugin Git Source Control VCS</tags>
    <releaseNotes>
      - Fixed a bug where LibGit2Sharp.NativeBinaries wouldn't be installed. The plugin will now try to copy the required files from the local package source.
    </releaseNotes>
    <dependencies>
        <dependency id="AdamsLair.Duality" version="2.7.3" />
        <dependency id="AdamsLair.Duality.Editor" version="2.6.2" />
        <dependency id="LibGit2Sharp" version="0.22.0" />
    </dependencies>
  </metadata>
  <files>
      <file src="..\EditorPlugin\bin\Release\GitPlugin.editor.dll" target="lib" />
      <file src="..\EditorPlugin\bin\Release\GitPlugin.editor.pdb" target="lib" />
      <file src="..\EditorPlugin\bin\Release\GitPlugin.editor.xml" target="lib" />
  </files>
</package>
